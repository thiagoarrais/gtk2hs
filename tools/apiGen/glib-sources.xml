<gapi-parser-input>
  <api filename="glib-api.xml">
    <library name="glib">
      <namespace name="G">
        <dir>glib/glib</dir>
      	<!-- exclude huge static arrays -->
        <exclude>glib/glib/gscanner.h</exclude>
        <exclude>glib/glib/gunibreak.h</exclude>
        <exclude>glib/glib/gunichartables.h</exclude>
        <exclude>glib/glib/gunicomp.h</exclude>
        <exclude>glib/glib/gunidecomp.h</exclude>

        <!-- this file causes the gapi parser to go into a infinite loop -->
        <exclude>glib/glib/gbsearcharray.h</exclude>
      </namespace>
    </library>
  </api>
</gapi-parser-input>
